{
  "name": "size-limit-action",
  "version": "1.7.0",
  "private": true,
  "description": "size-limit action",
  "main": "dist/index.js",
  "license": "ISC",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "build": "ncc build src/main.ts -o dist",
    "size-build": "npm run build",
    "size": "npm run size-build && size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "4500 ms"
    }
  ],
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/exec": "^1.0.3",
    "@actions/github": "^5.0.3",
    "bytes": "^3.1.0",
    "has-pnpm": "^1.1.1",
    "has-yarn": "^3.0.0",
    "markdown-table": "^3.0.2"
  },
  "devDependencies": {
    "@size-limit/file": "^8.0.0",
    "@size-limit/time": "^8.0.0",
    "@types/bytes": "^3.1.1",
    "@types/jest": "^28.1.6",
    "@types/markdown-table": "^3.0.0",
    "@types/node": "^12.7.12",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.21.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^26.7.0",
    "jest": "^28.1.3",
    "jest-circus": "^28.1.3",
    "js-yaml": "^3.13.1",
    "prettier": "^2.7.1",
    "size-limit": "^8.0.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
